use crate.Unwrap
use crate.Div
use crate.panic

pub enum Option<T> {
	Some(T),
	None,
}

impl<T> Unwrap<T> for Option<T> {
	func unwrap(move self) -> T, @Div {
		match self {
			Some(value) => value,
			None => panic("Called Option.unwrap() on Option.None"),
		}
	}

	func expect(move self, panic_msg: String) -> T, @Div {
		match self {
			Some(value) => value,
			None => panic(panic_msg),
		}
	}
}
