const fib = fn(n)
    if n == 0 then 0
    else if n == 1 then 1
    else fib(n - 1) + fib(n - 2);

const main = fn() {
    assert_eq(fib(0), 0);
    assert_eq(fib(1), 1);
    assert_eq(fib(2), 1);
    assert_eq(fib(3), 2);
    assert_eq(fib(4), 3);
    assert_eq(fib(5), 5);
    assert_eq(fib(6), 8);
    assert_eq(fib(7), 13);
    assert_eq(fib(8), 21);
    assert_eq(fib(9), 34);
    assert_eq(fib(10), 55);
    assert_eq(fib(20), 6765);
};
